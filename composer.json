{
  "name":              "fernleafsystems/integrations-edd",
  "description":       "Provides useful helper classes for Easy Digital Downloads",
  "authors":           [
    {
      "name":     "Paul Goodchild",
      "email":    "paul@paulgoodchild.me",
      "homepage": "https://paulgoodchild.me"
    }
  ],
  "license":           "MIT",
  "keywords":          [
    "Easy Digital Downloads",
    "FreeAgent",
    "Stripe",
    "PayPal",
    "Utilities"
  ],
  "repositories":      [
  ],
  "require":           {
    "php":                                    ">=7.4",
    "fernleafsystems/integrations-freeagent": "^2.1",
    "fernleafsystems/wordpress-services":     "^3.0",
    "ext-json":                               "*",
    "ext-bcmath":                             "*"
  },
  "require-dev":       {
  },
  "minimum-stability": "dev",
  "prefer-stable":     true,
  "autoload":          {
    "psr-4": {
      "FernleafSystems\\Integrations\\Edd\\": "src"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": false
    }
  }
}
